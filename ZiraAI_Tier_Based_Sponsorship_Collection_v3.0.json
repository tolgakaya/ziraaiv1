      "type": "string"
    },
    {
      "key": "sponsorId",
      "value": "",
      "type": "string"
    },
    {
      "key": "farmerId",
      "value": "",
      "type": "string"
    },
    {
      "key": "profileId",
      "value": "",
      "type": "string"
